{
    "name": "plenigo/plenigo-php-sdk",
    "type": "library",
    "description": "Plenigo PHP SDK",
    "keywords": ["paywall","pay"],
    "homepage": "https://www.plenigo.com",
    "autoload": {
        "files": ["src/plenigo/Plenigo.php"],
